{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
  "typeRoots": ["src/@types"],
  "baseUrl": ".",
  "paths": {
    "src/*": ["src/*"],
    "types/*": ["src/@types/*"],
    "assets/*": ["src/assets/*"],
    "navigation/*": ["src/navigation/*"],
    "screens/*": ["src/screens/*"],
    "utils/*": ["src/utils/*"],
  }
}
}
